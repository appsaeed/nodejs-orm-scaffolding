    "@types/express-session": "^1.18.0",
    "@types/node": "^20.12.7",
    "colorette": "^2.0.20",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express-session": "^1.18.0",
    "knex": "^3.1.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "express": "^4.19.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.4"
  }
}
