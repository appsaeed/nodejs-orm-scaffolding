{
  "name": "nodejs-orm-scaffolding",
  "version": "1.0.0",
  "description": "",
  "main": "main.ts",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "env": "node ./app/base/env.cli.js",
    "dev": "node ./app/base/env.cli.js && tsx watch main.ts",
    "help": "tsx ./app/base/db.cli.js --help",
    "migrate:make": "tsx ./app/base/db.cli.js migrate:make",
    "migrate:up": "tsx ./app/base/db.cli.js migrate:up",
    "migrate:latest": "tsx ./app/base/db.cli.js migrate:latest",
    "migrate:rollback": "tsx ./app/base/db.cli.js migrate:rollback",
    "migrate:down": "tsx ./app/base/db.cli.js migrate:down",
    "migrate:list": "tsx ./app/base/db.cli.js migrate:list",
    "migrate:status": "tsx ./app/base/db.cli.js migrate:status",
    "migrate:version": "tsx ./app/base/db.cli.js migrate:version",
    "migrate:unlock": "tsx ./app/base/db.cli.js migrate:unlock",
    "seed:make": "tsx ./app/base/db.cli.js seed:make",
    "seed:run": "tsx ./app/base/db.cli.js seed:run",
    "migrate:versions": "tsx ./app/base/db.cli.js migrate:version"
  },
  "keywords": [],
  "author": "",
